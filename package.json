{
  "name": "sherby-metadata",
  "version": "1.0.1",
  "description": "Manage meta tags for Search Engine Optimization (SEO)",
  "author": "Benjamin Rancourt",
  "license": "MIT",
  "repository": "SherbyElements/sherby-metadata",
  "bugs": {
    "url": "https://github.com/SherbyElements/sherby-metadata/issues"
  },
  "homepage": "https://github.com/SherbyElements/sherby-metadata",
  "devDependencies": {
    "eslint": "^4.5.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-json": "^1.2.0"
  },
  "scripts": {
    "format": "npm run format:js && npm run format:json",
    "format:js": "eslint . --ext js,html --ignore-path .gitignore --fix",
    "format:json": "eslint . --ext .json --ignore-path .gitignore --fix",
    "lint": "npm run lint:js && npm run lint:json && npm run lint:polymer",
    "lint:js": "eslint . --ext js,html --ignore-path .gitignore",
    "lint:json": "eslint . --ext .json --ignore-path .gitignore",
    "lint:polymer": "polymer lint",
    "test": "polymer test"
  },
  "keywords": [
    "description",
    "keywords",
    "meta-tags",
    "meta",
    "metadata",
    "open-graph",
    "polymer",
    "seo",
    "sherby",
    "title",
    "webcomponents"
  ]
}
